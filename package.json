    "cspell": "^9.6.4",
    "cypress": "^15.9.0",
    "drizzle-kit": "^0.31.8",
    "eslint": "^9",
    "eslint-config-next": "16.1.6",
    "eslint-plugin-cypress": "^5.2.1",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-vitest": "^0.5.4",
    "happy-dom": "^20.5.0",
