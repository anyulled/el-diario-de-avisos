    "tailwindcss": "^4",
    "tsx": "^4.21.0",
    "typescript": "^5",
    "vitest": "^4.0.16"
  },
  "overrides": {
    "esbuild": "0.27.2"
  },
  "lint-staged": {
    "!(cypress)/**/*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "vitest related --run --passWithNoTests",
      "prettier --write"
    ],
    "*.{json,css,md}": [
      "prettier --write"
    ]
  },
  "name": "web",
  "private": true,
  "scripts": {
    "build": "next build",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "dev": "next dev",
    "ingest": "npx tsx scripts/ingest.ts",
    "lint": "eslint",
    "lint:docs": "markdownlint '**/*.md' --ignore node_modules",
    "spell-check": "cspell '**/*.md'",
    "prepare": "husky",
    "start": "next start",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "test:e2e": "start-server-and-test dev http://localhost:3000 cypress:run",
    "test:integration": "vitest run src/**/*.integration.test.ts",
    "test:ui": "vitest --ui",
    "test:unit": "vitest run --exclude '**/*.integration.test.ts'",
    "transform-dates": "npx tsx scripts/transform-dates.ts",
    "transform-dates:dry-run": "npx tsx scripts/transform-dates.ts --dry-run",
    "transform-dates:rename": "npx tsx scripts/transform-dates.ts --rename",
    "type-check": "tsc --noEmit"
  },
  "version": "0.1.0"
}
